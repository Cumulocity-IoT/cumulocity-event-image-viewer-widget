    "e2e": "ng e2e",
    "buildPatch": "cd projects/gp-event-image-viewer && npm version patch && ng build gp-event-image-viewer && cd ../../dist/gp-event-image-viewer && npm pack && move *.tgz ../",
    "buildMinor": "cd projects/gp-event-image-viewer && npm version minor && ng build gp-event-image-viewer && cd ../../dist/gp-event-image-viewer && npm pack && move *.tgz ../",
    "buildMajor": "cd projects/gp-event-image-viewer && npm version major && ng build gp-event-image-viewer && cd ../../dist/gp-event-image-viewer && npm pack && move *.tgz ../",
    "serve": "ng build gp-event-image-viewer && npm i dist/gp-event-image-viewer && ng s",
    "runtime": "gulp --gulpfile ./runtime/gulpfile.js"
  },
  "private": true,
